  <Code>  Private Const Thau1 = " ألف "
    Private Const Thau2 = " ألفان "
    Private Const Thau = " آلاف "
    Private Const Thau3 = " ألفاً "
    Private Const thau4 = " ألفين "
    Private Const mill1 = "مليون"
    Private Const mill2 = "مليونين"
    Private Const mills = "ملايين"
  
    Private Lira As String = "ليرة "
    Private LengthNum As Integer = 0
    Private Nand As String = " و "
    Public Function NumToTxt(ByVal Number As String) As String
        Dim result As String = ""
        Dim re As String = ""
        Select Case Number.Length
            Case 1
                result = GetFistN(Number, Number)
            Case 2
                result = GetFistN(Mid(Number, 2, 1), Number) + IIf(Mid(Number, 2, 1) = 0 Or Mid(Number, 1, 1) = 1 Or Mid(Number, 1, 1) = 0, " ", Nand) + GetSecondN(Mid(Number, 1, 1))
            Case 3
                result = GetTherdN(Mid(Number, 1, 1)) + IIf(Mid(Number, 2, 1) &lt;&gt; 0 Or Mid(Number, 3, 1) &lt;&gt; 0, Nand, "") + GetFistN(Mid(Number, 3, 1), Number) _
                        + IIf(Mid(Number, 3, 1) = 0 Or Mid(Number, 2, 1) = 1 Or Mid(Number, 2, 1) = 0, " ", Nand) + GetSecondN(Mid(Number, 2, 1))
            Case 4
                result = Th(Number.Length, Number) &amp; GetTherdN(Mid(Number, 2, 1)) &amp; IIf(Mid(Number, 3, 1) &lt;&gt; 0 Or Mid(Number, 4, 1) &lt;&gt; 0, Nand, "") &amp; _
                        GetFistN(Mid(Number, 4, 1), Number) &amp; IIf(Mid(Number, 4, 1) = 0 Or Mid(Number, 3, 1) = 1 Or Mid(Number, 3, 1) = 0, " ", Nand) &amp; GetSecondN(Mid(Number, 3, 1))
            Case 5
                result = Th(Number.Length, Number) &amp; GetTherdN(Mid(Number, 3, 1)) &amp; IIf(Mid(Number, 3, 1) &lt;&gt; 0 And (Mid(Number, 4, 1) &lt;&gt; 0 Or Mid(Number, 5, 1) &lt;&gt; 0), Nand, "") &amp; _
                         GetFistN(Mid(Number, 5, 1), Number) &amp; IIf(Mid(Number, 5, 1) = 0 Or Mid(Number, 4, 1) = 1 Or Mid(Number, 4, 1) = 0, " ", Nand) &amp; GetSecondN(Mid(Number, 4, 1))
            Case 6
                result = Th(Number.Length, Number) &amp; GetTherdN(Mid(Number, 4, 1)) &amp; IIf(Mid(Number, 4, 1) &lt;&gt; 0 And (Mid(Number, 5, 1) &lt;&gt; 0 Or Mid(Number, 6, 1) &lt;&gt; 0), Nand, "") &amp; _
                         GetFistN(Mid(Number, 6, 1), Number) &amp; IIf(Mid(Number, 6, 1) = 0 Or Mid(Number, 5, 1) = 1 Or Mid(Number, 5, 1) = 0, " ", Nand) &amp; GetSecondN(Mid(Number, 5, 1))
            Case 7
                MsgBox("إذا أردت النسخة الكاملة إتصل على الرقم " + "+963-944-388282", vbOKOnly + vbInformation, "النسخة الحالية محدودة")
        End Select

        If Number.Length = 1 Then
            Select Case Number
                Case 1
                    Lira = "ليرة"
                    re = "فقط"+ "ليرة سورية واحدة لا غير "
                Case 2
                    Lira = "ليرتان"
                    re = " فقط " + " " + Lira + "سورية لاغير"
                Case Is &gt; 2
                    Lira = "ليرات"
                    re = " فقط "+ " " + result + " " + Lira + "سورية لا غير"
            End Select
        ElseIf Number.Length = 2 Then
            If Number = "10" Then
                Lira = "ليرات"
            Else
                Lira = "ليرة"
            End If
            re = " فقط "+ " " + result + " " + Lira + "  سوريةلا غير "
        ElseIf Number.Length &gt; 2 Then
            Lira = "ليرة"
            Dim sp As String = Strings.Right(Number, 2)
            Select Case CInt(sp)
                Case 1
                    re = " فقط "+ result + " " + "ليرة سورية لا غير "
                Case 2
                    re = " فقط "+ result + " " + "ليرة سورية لا غير "
                Case Is &gt; 2
                    re = " فقط "+ result + " " + "ليرات سورية لا غير "
            End Select
            If sp &gt; 10 Then
                   re = " فقط "+ result + " " + "ليرة سورية لا غير "
	else
	   Lira = "ليرة"
           	   re = " فقط "+ " " + result + " " + Lira + " سورية لا غير "
            End If
        Else
            Lira = "ليرة"
            re = " فقط "+ " " + result + " " + Lira + " سورية لا غير "
        End If

        Return re
    End Function
    Private Function GetFistN(ByRef LN As String, ByVal nm As String) As String
        Dim result As String = ""
        Select Case LN
            Case 1
                If Mid(nm, IIf(nm.Length = 1, 2, nm.Length) - 1, 1) = 1 Or nm.Length = 5 Then
                    result = "أحد"
                Else
                    result = "واحد"
                End If
            Case 2
                If Mid(nm, IIf(nm.Length = 1, 2, nm.Length) - 1, 1) = 1 And nm.Length = 2 Or nm.Length = 5 Then
                    result = "إثنا"
                Else
                    result = "إثنان"
                End If
            Case 3
                result = "ثلاثة"
            Case 4
                result = "أربعة"
            Case 5
                result = "خمسة"
            Case 6
                result = "ستة"
            Case 7
                result = "سبعة"
            Case 8
                result = "ثمانية"
            Case 9
                result = "تسعة"
        End Select
        Return result
    End Function
    Private Function GetSecondN(ByRef LN As String) As String
        Dim result As String = ""
        Select Case LN
            Case 1
                result = "عشرة"
            Case 2
                result = "عشرون"
            Case 3
                result = "ثلاثون"
            Case 4
                result = "أربعون"
            Case 5
                result = "خمسون"
            Case 6
                result = "ستون"
            Case 7
                result = "سبعون"
            Case 8
                result = "ثمانون"
            Case 9
                result = "تسعون"
        End Select
        Return result
    End Function
    Private Function GetTherdN(ByRef LN As String) As String
        Dim result As String = ""
        Select Case LN
            Case 1
                result = "مئة"
            Case 2
                result = "مئتان"
            Case 3
                result = "ثلاثمائة"
            Case 4
                result = "أربعمائة"
            Case 5
                result = "خمسمائة"
            Case 6
                result = "ستمائة"
            Case 7
                result = "سبعمائة"
            Case 8
                result = "ثمانمائة"
            Case 9
                result = "تسعمائة"
        End Select
        Return result
    End Function
    Private Function Th(ByVal Countn As Integer, ByVal Cnumber As String) As String
        Dim result As String = ""
        If Countn = 4 Then
            If Mid(Cnumber, 1, 1) = 1 Then
                result = Thau1 &amp; IIf(Mid(Cnumber, 2, 1) = 0, "", Nand)
            ElseIf Mid(Cnumber, 1, 1) = 2 Then
                result = Thau2 &amp; IIf(Mid(Cnumber, 2, 1) = 0, "", Nand)
            Else
                result = GetFistN(Mid(Cnumber, 1, 1), Cnumber) &amp; " " &amp; Thau &amp; IIf(Mid(Cnumber, 2, 1) = 0, " ", Nand)
            End If
        End If
        If Countn = 5 Then
            If Mid(Cnumber, 1, 1) = 1 And Mid(Cnumber, 2, 1) = 0 Then
                result = GetSecondN(Mid(Cnumber, 1, 1)) &amp; " " &amp; Thau &amp; IIf(Mid(Cnumber, 2, 1) = 0 And Mid(Cnumber, 3, 1) = 0 And Mid(Cnumber, 4, 1) = 0 And Mid(Cnumber, 5, 1) = 0, " ", Nand)
            Else
                result = GetFistN(Mid(Cnumber, 2, 1), Cnumber) &amp; IIf(Mid(Cnumber, 2, 1) = 0 Or Mid(Cnumber, 1, 1) = 1 Or Mid(Cnumber, 1, 1) = 0, " ", Nand) &amp; GetSecondN(Mid(Cnumber, 1, 1)) &amp; " " &amp; Thau3 &amp; IIf(Mid(Cnumber, 2, 1) = 0 And Mid(Cnumber, 3, 1) = 0, " ", Nand)
            End If
        End If
        If Countn = 6 Then
            If Mid(Cnumber, 2, 1) = 0 And Mid(Cnumber, 3, 1) = 0 And Mid(Cnumber, 4, 1) = 0 And Mid(Cnumber, 5, 1) = 0 And Mid(Cnumber, 6, 1) = 0 Then
                result = GetTherdN(Mid(Cnumber, 1, 1)) + Thau1 + " "
            ElseIf Mid(Cnumber, 2, 1) = 0 And Mid(Cnumber, 3, 1) = 0 Then
                result = GetTherdN(Mid(Cnumber, 1, 1)) + Thau1 + Nand
            ElseIf Mid(Cnumber, 2, 1) = 0 And Mid(Cnumber, 3, 1) &lt;&gt; 0 Then
                result = GetTherdN(Mid(Cnumber, 1, 1)) + Nand + IIf(Mid(Cnumber, 3, 1) = 1, Thau1, IIf(Mid(Cnumber, 3, 1) = 2, thau4, GetFistN(Mid(Cnumber, 3, 1), Cnumber) + Thau)) + IIf(Mid(Cnumber, 4, 1) = 0 And Mid(Cnumber, 5, 1) = 0 And Mid(Cnumber, 6, 1) = 0, "", Nand)
            Else
                result = GetTherdN(Mid(Cnumber, 1, 1)) + Nand + GetFistN(Mid(Cnumber, 3, 1), Cnumber) + IIf(Mid(Cnumber, 3, 1) = 0 Or Mid(Cnumber, 2, 1) = 1 Or Mid(Cnumber, 2, 1) = 0, " ", Nand) + GetSecondN(Mid(Cnumber, 2, 1)) + Thau3 + Nand
            End If
        End If
        Return result
    End Function</Code>
